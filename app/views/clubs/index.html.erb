<% @clubs.each do |club| %>
  <div class='club row'>
    <h4 class='col-lg-2'><%= club.name %> <small><%= club.category %></small></h4>
    <p class='col-lg-8'>
      <%= Faker::Lorem.paragraph %>
    </p>
    <div class=' col-lg-2 actions'>
      <%= button_to "Choose", "/", class: "btn btn-success"  %>
    </div>
  </div>
<% end %>
